{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket",
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::mag-raw",
        "arn:aws:s3:::mag-raw/*",
        "arn:aws:s3:::mag-processed",
        "arn:aws:s3:::mag-processed/*",
        "arn:aws:s3:::euv-raw",
        "arn:aws:s3:::euv-raw/*",
        "arn:aws:s3:::euv-processed",
        "arn:aws:s3:::euv-processed/*"
      ]
    }
  ]
}